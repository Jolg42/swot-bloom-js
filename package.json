{
  "name": "swot-bloom-js",
  "version": "1.0.3",
  "description": "js implementation of swot using a bloom filter",
  "main": "dist/client.bundle.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "build": "webpack",
    "generateFilter": "babel-node ./src/buildFilter.js",
    "coverage": "istanbul cover _mocha",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 50 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "patch-release": "npm version patch && npm publish && git push --follow-tags"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "codecov.io": "0.1.6",
    "eslint": "^3.9.1",
    "eslint-config-airbnb-base": "^9.0.0",
    "eslint-plugin-import": "^2.1.0",
    "istanbul": "0.4.5",
    "json-loader": "^0.5.4",
    "mocha": "^1.21.4",
    "webpack": "^2.1.0-beta.25",
    "chai": "^1.9.2",
    "walk": "^2.3.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "bloomfilter": "0.0.16"
  }
}
